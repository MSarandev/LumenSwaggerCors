{
  "name": "maxim-sarandev/lumen-swagger-cors",
  "description": "CORS configuration added to the base Swagger Lumen repo",
  "keywords": ["lumen", "laravel", "swagger", "cors"],
  "license": "MIT",
  "version": "1.2",
  "authors": [
    {
      "name": "Darius Matulionis",
      "email": "darius@matulionis.lt"
    },
    {
      "name": "Maxim Sarandev",
      "email": "maxsarandev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/lumen-framework": "~5.6",
    "zircote/swagger-php": "~2.0|3.*",
    "swagger-api/swagger-ui": "^3.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0",
    "satooshi/php-coveralls": "^2.0",
    "vlucas/phpdotenv": "~3.3"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "SwaggerLume\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
